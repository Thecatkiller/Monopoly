<!DOCTYPE html>
<html>

<!-- HEAD -->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <meta name="description" content="Banco Pichincha" />
    <meta Http-Equiv="Cache-Control" Content="no-cache" />
    <meta Http-Equiv="Pragma" Content="no-cache" />
    <meta Http-Equiv="Expires" Content="0" />
    <meta name="format-detection" content="telephone=no">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="Page-Enter" content="blendTrans(Duration=0.5)" />
    <meta http-equiv="Page-Exit" content="blendTrans(Duration=0.5)" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>BANCO PICHINCHA</title>
    <!-- FAVICON -->
    <link rel="icon" type="image/x-icon" href="@Url.Content("~/Resources/Static/icon/favicon.ico")" />
    <!-- FIN DE FAVICON -->
    <!-- ESTILOS CSS DEL BANCO FINANCIERO -->
    @Styles.Render("~/Resources/Static/Css/General/style")
    @Styles.Render("~/Resources/Static/Css/General/custom")
    @Styles.Render("~/Resources/Static/Css/General/mCustomScrollbar")
    @Styles.Render("~/Resources/Static/Css/General/flipclock")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/materialize")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/library")

</head>
<body>
   

    <!-- CONTAINER CAMPONENTES INICIO-->
    <div class="container max640heightcontainer">
        <div class="row">
            <!-- MAIN FINANCIERO -->
            <div id="renderBody" class="col-md-12">
                @RenderBody()
            </div>
            <!-- FIN MAIN FINANCIERO -->
        </div>
    </div>
    <!-- FIN DE CONTAINER CAMPONENTES INICIO-->
    <!-- OVERLIGHT -->
    <div class="container_overlight"></div>
    <div class="container_overlight2"></div>
    <div class="container_overlight_menu"></div>
    <!-- FIN DE OVERLIGHT -->
    <!-- BARRA LATERAL -->
    <div class="help">

        <!-- //////////////////// ICONOS //////////////////// -->
        <div class="help__iconsContainer">
            <div class="help__icon help__iconBanca"><div class="help__avatar"><i class="icon-ico-reloj"></i></div></div>
            @*<div class="help__icon help__iconHablemos"><i class="icono-phone"></i></div>*@
            @*<div class="help__icon help__iconChat"><i class="icono-chat"></i></div>*@
        </div>
        <!-- //////////////////// CAJA CON TEXTOS //////////////////// -->
        <div class="help__hover">
            <ul class="help__hoverLista">
                <li class="help__hoverItem help__hoverItemBanca">MI SESIÓN</li>
                @*<li class="help__hoverItem help__hoverItemHablemos">HABLEMOS</li>*@
                @*<li class="help__hoverItem help__hoverItemChat">CHAT EN LÍNEA</li>*@
            </ul>
        </div>


        <!-- BLOQUE BANCA POR INTERNET -->

        <div id="back-top">
            <a href="#top"><i class="iconangle-double-up"></i></a>
        </div>

    </div>

    <div class="help__overlay"></div>
    <!-- FIN BARRA LATERAL -->
    <!-- Modales -->

    <div id="desconectar" class="modal fade modal-mensaje">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="modal-close close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Desconectar</h4>
                </div>
                <div class="modal-body">
                    <div class="error_transaccion">
                        <h4 class="text-center" style="font-size: 18px;">
                            @*GlobalVariables.CM_VIEW_HOME_INDEX_DESCONECTAR_BUTTON*@
                        </h4>
                    </div>
                    <div style="margin-bottom: -30px; ">
                        <ul class="buttonsdisconnect">
                            <li>
                                <a href="javascript:_desconectLogin();" class="boton-general" style="width: 180px; vertical-align: middle;padding: 8px 0px;">Desconectarme</a>
                            </li>
                            <li>
                                <a href="javascript:_Closedesconect();" class="boton-secundario" style="width: 180px; vertical-align: middle;padding: 6px 0px;">Seguir conectado</a>
                            </li>
                        </ul>
                    </div>
                    <br>
                    <br>
                    <br>
                </div>
            </div>
        </div>
    </div>

    <div id="ModalMessage" class="modal fade modal-mensaje">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Confirmación</h4>
                </div>
                <div class="modal-body">
                    <div class="error_transaccion" id="DivMessage">
                    </div>
                    <br>
                    <br>
                    <br>
                </div>
            </div>
        </div>
    </div>

    <div id="ModalError" class="modal fade modal-mensaje">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Mensaje de Error</h4>
                </div>
                <div class="modal-body">
                    <div class="error_transaccion">
                        <h4 class="text-center">
                            <span id="messageModalError"></span>
                        </h4>
                    </div>
                    <br>
                    <br>
                    <br>
                </div>
            </div>
        </div>
    </div>

    <div id="ModalLoading" class="modal fade modal-mensaje">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Procesando Transacción</h4>
                </div>
                <div class="modal-body">
                    <div class="error_transaccion">
                        Procesando
                        <br />
                        <h4 class="text-center">
                            <span id="messageModalLoading"></span>
                        </h4>
                    </div>
                    <br>
                    <br>
                    <br>
                </div>
            </div>
        </div>
    </div>

    <!-- NEW MODAL LOADING -->
    <div class="mLoading">
        <div class="mLoadingContent">
            <img src="@Url.Content("~/Resources/img/loading-circle.gif")" />
            <p class="text"></p>
        </div>
    </div>

    @Scripts.Render("~/bundles/custom")
    @Scripts.Render("~/bundles/Layout")
    @Scripts.Render("~/bundles/LayoutApp")
    @RenderSection("ScriptsView", false)

    <script src="~/Scripts/jquery.signalR-2.4.1.js"></script>
    <script src="~/signalr/hubs"></script>

    <script>
                /*$(function () {
                    //Referencia al proxy autogenerado para el hub (concentrador).
                    var sessionServer = $.connection.sessionHub;
                    sessionServer.client.checkNewSession = function (sessionId) {

                    };
                    $.connection.hub.start().done(function () {

                    });
                });*/
    </script>

</body>
</html>

